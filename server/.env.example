NAPKIN_TOKEN=your_napkin_api_token_here

# AI Provider Selection (true/false)
# Set USE_OPENAI=true to use OpenAI, false (or omit) to use Gemini
USE_OPENAI=false

# API Keys (set the one you want to use)
OPENAI_API_KEY=your_openai_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here

# Model Selection (optional - uses defaults if not set)
# OpenAI models: gpt-4o-mini, gpt-4o, gpt-4-turbo, gpt-3.5-turbo
# Gemini models: gemini-2.5-flash, gemini-2.5-pro, gemini-3-flash-preview, gemini-3.1-pro-preview
OPENAI_MODEL=gpt-4o-mini
GEMINI_MODEL=gemini-2.5-flash

PORT=3000
